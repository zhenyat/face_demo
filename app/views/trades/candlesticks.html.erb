<% content_for :google_candlesticks_scripts do %>
  <%= javascript_include_tag "candlesticks.js" %>
<% end %>
<h5>
  <%= "Time elapsed: #{@time_elapsed} seconds" %>
</h5>
<hr/>
<h1>
  <%= "WEX Candlesticks (priceMin - priceMax, priceFirst - priceLast).  Time Slot: #{TIME_SLOT / 60} min" %>
</h1>
<% @pairs.each do |pair| %>
  <div id="<%= pair %>_chart_div"></div>
<% end %>
